AtBA;AAVA,CAAA","sourcesContent":["<template>\r\n    <div class=\"contact\">\r\n        <a target=\"blank\" href=\"https://github.com/tustica\"><img id=\"github\" src=\"../assets/github.png\" alt=\"github\"></a>\r\n        <a target=\"blank\" href=\"https://www.linkedin.com/in/tim-ustica-3ba08816b/\"><img id=\"linkedin\" src=\"../assets/linkedin.png\" alt=\"linkedin\"></a>\r\n        <h1>Get in Contact!</h1>\r\n        <form id=\"form\">\r\n                <input class=\"content\" type=\"text\" name=\"name\" v-model=\"name\" placeholder=\"Name\">\r\n                <input class=\"content\" type=\"text\" name=\"email\" v-model=\"email\" placeholder=\"Email\">\r\n                <input class=\"content\" type=\"text\" name=\"company\" v-model=\"company\" placeholder=\"Company\">\r\n                <textarea class=\"content\" cols=\"30\" rows=\"5\" name=\"message\" v-model=\"message\" placeholder=\"Message\"></textarea>\r\n                <input @click=\"sendEmail()\" id=\"send\" class=\"content\" type=\"submit\" value=\"Send\">\r\n        </form>\r\n    </div> \r\n</template>\r\n\r\n<script>\r\nimport emailjs from 'emailjs-com';\r\nimport{ init } from 'emailjs-com';\r\ninit(\"user_3gqTY4G8Rbn56e3Gd2uBT\");\r\nexport default {\r\n    name: 'Contact',\r\n    data(){\r\n        return {\r\n            name: '',\r\n            email: '',\r\n            company: '',\r\n            message: ''\r\n        }\r\n    },\r\n    methods: {\r\n        sendEmail(e){\r\n            e.preventDefault();\r\n            try {\r\n                emailjs.sendForm('service_r3jggdq', 'template_hreugpi', e.target,\r\n                'user_3gqTY4G8Rbn56e3Gd2uBT', {\r\n                    name: this.name,\r\n                    email: this.email,\r\n                    company: this.company,\r\n                    message: this.message\r\n                })\r\n                .then(function() {\r\n                        console.log('SUCCESS!')\r\n                })\r\n            }\r\n            catch(error) {\r\n                console.log({error})\r\n            }\r\n            this.name = ''\r\n            this.email = ''\r\n            this.company = ''\r\n            this.message = ''\r\n        }\r\n    }\r\n\r\n}\r\n</script>\r\n<style lang='scss' scoped>\r\n    $tablet: 768px;\r\n    $phone: 425px;\r\n    $mphone: 375px;\r\n    $sphone: 320px;\r\n\r\n    .contact{\r\n        width: 100vw;\r\n        height: 130vh;\r\n        background-color: #313a89;\r\n        h1{\r\n            color: white;\r\n            font-size: 60px;\r\n            padding: 25px;\r\n            margin: 0px;\r\n            font-family: 'Montserrat', sans-serif;\r\n        }\r\n        #form{\r\n            display: flex;\r\n            flex-direction: column;\r\n            padding: 5px 500px;\r\n\r\n            .content{\r\n                border-radius: 10px;\r\n                border: none;\r\n                font-size: 25px;\r\n                padding: 10px;\r\n                margin: 10px;\r\n            }\r\n            #send{\r\n                width: 40%;\r\n                cursor: pointer;\r\n                align-self: center;\r\n                color: white;\r\n                font-size: 30px;\r\n                font-family: 'Montserrat', sans-serif;\r\n                background-color: rgb(206, 58, 0);\r\n                transition: all .3s ease-in-out;\r\n            }\r\n            #send:hover{\r\n                background-color: rgba(255, 136, 0 0.644);\r\n            }\r\n        }\r\n    }\r\n    #github, #linkedin{\r\n        width: 150px;\r\n        height: 150px;\r\n        margin: 30px;\r\n        border-radius: 70%;\r\n        transition: .3s ease-in-out;\r\n    }\r\n    #github:hover, #linkedin:hover{\r\n        background-color: rgb(206, 58, 0);\r\n        cursor: pointer;\r\n    }\r\n    @media screen and (max-width: $tablet) {\r\n        .contact{\r\n            width: 100vw;\r\n            height: 900px;\r\n            overflow: hidden;\r\n            position: absolute;\r\n            top: 5550px;\r\n            h1{\r\n                padding: 40px;\r\n            }\r\n            #form{\r\n                padding: 20px 200px;\r\n            }\r\n        }\r\n    }\r\n    @media screen and (max-width: $phone){\r\n        .contact{\r\n            width: 100vw;\r\n            height: 1300px;\r\n            overflow: hidden;\r\n            position: absolute;\r\n            top: 5650px;\r\n            h1{\r\n                padding: 40px;\r\n            }\r\n            #form{\r\n                padding: 20px 70px;\r\n                #send{\r\n                    font-size: 23px;\r\n                }\r\n            }\r\n        }\r\n    }\r\n    @media screen and (max-width: $mphone){\r\n        .contact{\r\n            width: 100vw;\r\n            height: 1200px;\r\n            overflow: hidden;\r\n            position: absolute;\r\n            top: 5700px;\r\n            h1{\r\n                padding: 40px;\r\n            }\r\n            #form{\r\n                padding: 20px 50px;\r\n                #send{\r\n                    font-size: 23px;\r\n                }\r\n            }\r\n        }\r\n    }\r\n</style>\r\n"],"sourceRoot":"src/components"}]}